<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<GenerateDocumentationFile>True</GenerateDocumentationFile>
		<DocumentationFile>..\Milvonion.Domain\Milvonion.Domain.xml</DocumentationFile>
	</PropertyGroup>


	<PropertyGroup Condition="'$(Configuration)' == 'Debug'">
		<Nullable>disable</Nullable>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)' != 'Debug'">
		<Nullable>xnullablexx</Nullable>
	</PropertyGroup>

	
	<ItemGroup>
		<PackageReference Include="Milvasoft.Core.MultiLanguage" Version="8.2.2" />
		<PackageReference Include="Milvasoft.DataAccess.EfCore" Version="8.2.2" />
		<PackageReference Include="Milvasoft.Identity" Version="8.2.2" />
	</ItemGroup>

</Project>
